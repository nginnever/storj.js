{
  "name": "storj.js",
  "version": "0.0.0",
  "description": "Storj.io browser library",
  "main": "index.js",
  "directories": {},
  "scripts": {
    "build": "browserify lib/*.js > storj.es6.js # && babel storj.es6.js > storj.es5.js",
    "pretest": "jshint lib/*.js #&& browserify lib/*.js test/**/*.js -o test.js",
    "test": "nyc --reporter=text --reporter=html tape test/**/*.js #&& testling",
    "make-docs": "rm -rf ./jsdoc && ./node_modules/.bin/jsdoc lib -r -R README.md -c .jsdoc.json --verbose -d ./jsdoc"
  },
  "testling": {
    "files": [
      "lib/*.js",
      "test/**/*.js"
    ]
  },
  "author": "Chris Pollard",
  "contributors": [
    {
      "name": "retrohacker",
      "url": "https://github.com/retrohacker"
    }
  ],
  "license": "LGPL",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "browserify": "^13.1.1",
    "concat-stream": "^1.6.0",
    "ink-docstrap": "bookchin/docstrap",
    "jsdoc": "^3.4.3",
    "jshint": "^2.9.4",
    "nyc": "^10.0.0",
    "proxyquire": "^1.7.10",
    "tape": "^4.6.3",
    "testling": "^1.7.1"
  },
  "dependencies": {
    "chunk-store-stream": "^2.0.2",
    "filereader-stream": "^1.0.0",
    "from2": "^2.3.0",
    "memory-chunk-store": "^1.2.0",
    "readable-stream": "^2.2.2",
    "render-media": "^2.9.1",
    "storj-lib": "retrohacker/core",
    "stream-to-blob": "^1.0.0",
    "stream-to-blob-url": "^2.1.0"
  }
}
